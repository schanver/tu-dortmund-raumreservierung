{
"compilerOptions": {
  "rootDir": "./src",
  "outDir": "./dist",
  "module": "NodeNext",
  "target": "ES2022",
  "lib": ["ESNext", "DOM","DOM.Iterable"],
  "types": ["node"],
  "sourceMap": true,
  "declaration": true,
  "declarationMap": true,
  "strict": true,
  "noUncheckedIndexedAccess": true,
  "exactOptionalPropertyTypes": true,
  "isolatedModules": true,
  "skipLibCheck": true
}
}
